"pages/property-detail/property-detail.mappers.js","original":{"line":25,"column":29}},{"generated":{"line":38,"column":2},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":26,"column":4}},{"generated":{"line":38,"column":3},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":27,"column":2}},{"generated":{"line":38,"column":4},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":27,"column":3}},{"generated":{"line":39,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":28,"column":0}},{"generated":{"line":39,"column":1},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":28,"column":1}},{"generated":{"line":40,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":29,"column":0}},{"generated":{"line":41,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":30,"column":0}},{"generated":{"line":42,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":31,"column":0}},{"generated":{"line":43,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":32,"column":0}},{"generated":{"line":44,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":33,"column":0}},{"generated":{"line":45,"column":0},"source":"pages/property-detail/property-detail.mappers.js","original":{"line":34,"column":0}}],"sources":{"pages/property-detail/property-detail.mappers.js":"import { getRoomWord } from \"../property-list/property-list.mappers\"\r\nexport const getBathRoomsWord = bathroom => (bathroom > 1 ? 'baños' : 'baño');\r\n\r\n// ViewModel (modelado de datos)\r\nexport const propertyViewModel = (property,equipmentData)=> ({\r\nmainImage:property.images[0],\r\ntitle:property.title,\r\ncity:property.city,\r\nrooms:`${property.rooms} ${getRoomWord(property.rooms)}`,\r\nsquareMeter:`${property.squareMeter} m2`,\r\nbathrooms:`${property.bathrooms} ${getBathRoomsWord(property.bathrooms)}`,\r\nprice:`${property.price.toLocaleString()}€`,\r\nnotes:`${property.notes.substring(0,2000)}`,\r\nmainFeatures:property.mainFeatures,\r\nequipments:switchToEquipmentNames(property.equipmentIds,equipmentData),\r\nlocationUrl:property.locationUrl,\r\nimages:property.images,\r\n});\r\n\r\n//Funcion que permite buscar cuáles indices entre el array y el objeto coinciden.\r\n//Si coinciden, cambiar de Id (número) a \"nombre\" del equipamiento\r\nexport const switchToEquipmentNames = (equipmentArray, equipmentObject) => {\r\n  return equipmentArray.map(equipmentArrayIdValue => {\r\n    const findEquipment = equipmentObject.find(equipmentObjectIdValue => equipmentObjectIdValue.id === equipmentArrayIdValue);\r\n    return findEquipment.name;\r\n    }\r\n  )\r\n};\r\n/* Al array inicialmente se le aplica \".map\", a fin de poderse iterar y procesar sus índices,\r\nLuego, al objeto se le aplica \".find\", a fin de que encuentren coincidencias entre su Id \r\ny el id del array. Si se encuentra una coincidencia, se le pedirá a ese objeto resultante\r\nsu propiedad \".name.\"\" Y esta es la que se retorna. (.name) se utiliza para acceder \r\na la propiedad name de ese objeto específico.\r\n*/ \r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"6f69dcf9caad8aa6f9bb7970929bfe65","cacheData":{"env":{}}}